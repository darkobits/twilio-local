{
  "name": "@darkobits/twilio-local",
  "description": "Securely route Twilio applications to a local server.",
  "version": "0.2.1",
  "author": "darkobits",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkobits/twilio-local.git"
  },
  "keywords": [
    "development",
    "local",
    "localhost",
    "monitor",
    "nat",
    "ngrok",
    "nodemon",
    "reload",
    "restart",
    "tunnel",
    "tunneling",
    "twilio",
    "watch"
  ],
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.7.0"
  },
  "bin": {
    "twilio-local": "dist/bin/cli.js"
  },
  "main": "dist/twilio-local.js",
  "scripts": {
    "clean": "rimraf dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly --pretty; rimraf **/*.spec.d.ts",
    "test": "jest",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline",
    "prebump": "run-p test build",
    "bump": "standard-version"
  },
  "dependencies": {
    "@darkobits/log": "^1.1.0",
    "ajv": "^6.5.0",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.3",
    "camelcase": "^5.0.0",
    "chalk": "^2.4.1",
    "convert-hrtime": "^2.0.0",
    "cosmiconfig": "^5.0.5",
    "dotenv": "^5.0.1",
    "moment": "^2.22.2",
    "ngrok": "^3.0.1",
    "node-cleanup": "^2.1.2",
    "nodemon": "^1.17.5",
    "open": "0.0.5",
    "querystring": "^0.2.0",
    "signal-exit": "^3.0.2",
    "uuid": "^3.2.1",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-3": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/camelcase": "^4.1.0",
    "@types/cosmiconfig": "^5.0.1",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.0",
    "@types/node-cleanup": "^2.1.0",
    "@types/open": "0.0.29",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^11.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "2.6.x",
    "standard-version": "^4.4.0",
    "strip-ansi": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.9.1"
  }
}
